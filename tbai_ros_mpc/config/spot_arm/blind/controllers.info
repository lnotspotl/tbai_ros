sqpWbc
{
  ; Task weights for quadruped
  weightBaseTracking            1.0
  weightSwingLeg                200.0
  weightContactForce            0.001

  ; Task weights for arm
  weightArmEEPosition           1.0
  weightArmEEOrientation        0.5
  weightArmJointCentering       0.1

  ; Friction cone parameters
  frictionCoefficient     0.5

  ; Swing leg kp and kd (in cartesian space - ee tracking)
  swingKp                 350.0
  swingKd                 10.0

  ; Base position kp and kd
  baseKp                  1.0
  baseKd                  0.0

  ; Base orientation kp and kd
  eulerKp                 0.0
  eulerKd                 0.1

  ; Joint swing leg kp and kd
  jointSwingKp            50.0
  jointSwingKd            5.0

  ; Joint stance leg kp and kd
  jointStanceKp            50.0
  jointStanceKd            5.2

  ; Joint torque limit (leg)
  torqueLimit              80.0

  ; whether stance should be enforeced as a constraint
  ; or as a cost (false is more robust (e.g. slips))
  stanceAsConstraint false

  ; =============================================
  ; Arm WBC settings
  ; =============================================

  ; Arm end-effector position PD gains
  armEEPositionKp         100.0
  armEEPositionKd         20.0

  ; Arm end-effector orientation PD gains
  armEEOrientationKp      50.0
  armEEOrientationKd      10.0

  ; Arm joint centering (nullspace) PD gains
  armJointCenteringKp     10.0
  armJointCenteringKd     2.0

  ; Arm joint PD gains for motor commands
  armJointKp              200.0
  armJointKd              10.0

  ; Arm home position (stowed)
  armJointHomePosition
  {
    (0,0)  0.0    ; arm_sh0
    (1,0) -3.1    ; arm_sh1
    (2,0)  2.1    ; arm_el0
    (3,0)  0.0    ; arm_el1
    (4,0)  0.0    ; arm_wr0
    (5,0)  0.0    ; arm_wr1
  }

  ; Arm torque limits
  armTorqueLimits
  {
    (0,0)  45.0   ; arm_sh0
    (1,0)  45.0   ; arm_sh1
    (2,0)  30.0   ; arm_el0
    (3,0)  30.0   ; arm_el1
    (4,0)  15.0   ; arm_wr0
    (5,0)  15.0   ; arm_wr1
  }
}

